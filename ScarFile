		Util_CreateSquads(player8, sg_villagers, unit_villager_seven, InitSpawnVil7_2, Move_InitSpawnArmy7)	
		Util_CreateSquads(player8, sg_spearman, unit_spearman_seven, InitSpawnSpear7, Move_InitSpawnArmy7)
		Util_CreateSquads(player8, sg_spearman, unit_spearman_seven, InitSpawnSpear7, Move_InitSpawnArmy7)
