function art_move_start()
	Rule_AddInterval(art_move, 120)
end

function art_move()
		
	sg_art_one_1 = SGroup_CreateIfNotFound("sg_art_one_1")
	local Move_Art_One_POS = Util_GetPosition(Move_Art_One_1)
	Player_GetAllSquadsNearMarker(player2, sg_art_one_1, Move_Art_One_POS, 15)
	Cmd_AttackMove(sg_art_one_1, Move_Art_Two_1)
	
	sg_art_two_1 = SGroup_CreateIfNotFound("sg_art_two_1")
	local Move_Art_Two_POS = Util_GetPosition(Move_Art_Two_1)
	Player_GetAllSquadsNearMarker(player2, sg_art_two_1, Move_Art_Two_POS, 15)
	Cmd_AttackMove(sg_art_two_1, Move_Art_Three_1)
	
	sg_art_three_1 = SGroup_CreateIfNotFound("sg_art_three_1")
	local Move_Art_Three_POS = Util_GetPosition(Move_Art_Three_1)
	Player_GetAllSquadsNearMarker(player2, sg_art_three_1, Move_Art_Three_POS, 15)
	Cmd_AttackMove(sg_art_three_1, Move_Art_Four_1)
	
	sg_art_four_1 = SGroup_CreateIfNotFound("sg_art_four_1")
	local Move_Art_Four_POS = Util_GetPosition(Move_Art_Four_1)
	Player_GetAllSquadsNearMarker(player2, sg_art_four_1, Move_Art_Four_POS, 15)
	Cmd_AttackMove(sg_art_four_1, Move_Art_Five_1)
	
	sg_art_five_1 = SGroup_CreateIfNotFound("sg_art_five_1")
	local Move_Art_Five_POS = Util_GetPosition(Move_Art_Five_1)
	Player_GetAllSquadsNearMarker(player2, sg_art_five_1, Move_Art_Five_POS, 15)
	Cmd_AttackMove(sg_art_five_1, Move_Art_Six_1)
	
	sg_art_six_1 = SGroup_CreateIfNotFound("sg_art_six_1")
	local Move_Art_Six_POS = Util_GetPosition(Move_Art_Six_1)
	Player_GetAllSquadsNearMarker(player2, sg_art_six_1, Move_Art_Six_POS, 15)
	Cmd_AttackMove(sg_art_six_1, Move_Art_Seven_1)
			
	sg_art_one_2 = SGroup_CreateIfNotFound("sg_art_one_2")
	local Move_Art_One_POS = Util_GetPosition(Move_Art_One_2)
	Player_GetAllSquadsNearMarker(player2, sg_art_one_2, Move_Art_One_POS, 15)
	Cmd_AttackMove(sg_art_one_2, Move_Art_Two_2)
	
	sg_art_two_2 = SGroup_CreateIfNotFound("sg_art_two_2")
	local Move_Art_Two_POS = Util_GetPosition(Move_Art_Two_2)
	Player_GetAllSquadsNearMarker(player2, sg_art_two_2, Move_Art_Two_POS, 15)
	Cmd_AttackMove(sg_art_two_2, Move_Art_Three_2)
	
	sg_art_three_2 = SGroup_CreateIfNotFound("sg_art_three_2")
	local Move_Art_Three_POS = Util_GetPosition(Move_Art_Three_2)
