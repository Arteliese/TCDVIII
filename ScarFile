	Cmd_AttackMove(sg_art_three_2, Move_Art_Four_2)
	
	sg_art_four_2 = SGroup_CreateIfNotFound("sg_art_four_2")
	local Move_Art_Four_POS = Util_GetPosition(Move_Art_Four_2)
	Player_GetAllSquadsNearMarker(player2, sg_art_four_2, Move_Art_Four_POS, 15)
	Cmd_AttackMove(sg_art_four_2, Move_Art_Five_2)
	
	sg_art_five_2 = SGroup_CreateIfNotFound("sg_art_five_2")
