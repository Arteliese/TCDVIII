function first_player_deathball_spawns_start_1()
	Rule_AddOneShot(first_player_deathball_spawns_1, 120)
	local sg_villagers = SGroup_CreateIfNotFound("sg_villagers")
	Util_CreateSquads(player1, sg_villagers, unit_villager_one, InitSpawnVil1, Move_InitSpawn1)
end

function first_player_deathball_spawns_1()
	sg_first_player_deathball_1 = SGroup_CreateIfNotFound("sg_first_player_deathball_1")
	local first_deathball_ram = BP_GetSquadBlueprint("unit_ram_3_fre")
	local first_deathball_arc = BP_GetSquadBlueprint("unit_crossbowman_3_fre")
	local first_deathball_inf = BP_GetSquadBlueprint("unit_manatarms_1_eng")
	Util_CreateSquads(player2, sg_first_player_deathball_1, first_deathball_ram, Stream_One_1, Move_Stream_One_1)
	Util_CreateSquads(player2, sg_first_player_deathball_1, first_deathball_arc, Stream_One_2, Move_Stream_One_1)
	Util_CreateSquads(player2, sg_first_player_deathball_1, first_deathball_arc, Stream_One_3, Move_Stream_One_1)
	Util_CreateSquads(player2, sg_first_player_deathball_1, first_deathball_inf, Stream_One_4, Move_Stream_One_1)
	Util_CreateSquads(player2, sg_first_player_deathball_1, first_deathball_inf, Stream_One_5, Move_Stream_One_1)
	Util_CreateSquads(player2, sg_first_player_deathball_1, first_deathball_inf, Stream_One_6, Move_Stream_One_1)
end

