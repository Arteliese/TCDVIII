		unit_villager_seven = BP_GetSquadBlueprint("unit_villager_1_rus")
		unit_spearman_seven = BP_GetSquadBlueprint("unit_spearman_1_rus")
		unit_archer_seven = BP_GetSquadBlueprint("unit_archer_2_rus")
	elseif player_civ7 == "sultanate" then
		unit_villager_seven = BP_GetSquadBlueprint("unit_villager_1_sul")
		unit_spearman_seven = BP_GetSquadBlueprint("unit_spearman_1_sul")
		unit_archer_seven = BP_GetSquadBlueprint("unit_archer_2_sul")
	elseif player_civ7 == "abbasid" then
		unit_villager_seven = BP_GetSquadBlueprint("unit_villager_1_abb")
		unit_spearman_seven = BP_GetSquadBlueprint("unit_spearman_1_abb")
		unit_archer_seven = BP_GetSquadBlueprint("unit_archer_2_abb")
	end
		Util_CreateSquads(player8, sg_villagers, unit_villager_seven, InitSpawnVil7, Move_InitSpawn7)
		Util_CreateSquads(player8, sg_villagers, unit_villager_seven, InitSpawnVil7, Move_InitSpawn7)
