	Player_SetResource(player8, RT_Wood, 1200)
	Player_SetResource(player8, RT_Food, 1000)
	Player_SetResource(player8, RT_Stone, 400)
	Player_SetResource(player8, RT_Gold, 200)
	player_civ7 = Player_GetRaceName(player8)
	if player_civ7 == "english" then
		unit_villager_seven = BP_GetSquadBlueprint("unit_villager_1_eng")
		unit_spearman_seven = BP_GetSquadBlueprint("unit_archer_2_eng")
		unit_archer_seven = BP_GetSquadBlueprint("unit_archer_2_eng")
	elseif player_civ7 == "chinese" then
		unit_villager_seven = BP_GetSquadBlueprint("unit_villager_1_chi")
		unit_spearman_seven = BP_GetSquadBlueprint("unit_spearman_1_chi")
		unit_archer_seven = BP_GetSquadBlueprint("unit_archer_2_chi")
