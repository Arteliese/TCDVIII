import("ScarUtil.scar")	
import("squad.scar")

function OnInit()
	Rule_AddOneShot(Player_Count_Check, 3)
	player1 = World_GetPlayerAt(1)
	player2 = World_GetPlayerAt(2)
end

function Player_Count_Check()
	player_count = World_GetPlayerCount()
	if player_count == 2
		then
		Rule_AddOneShot(first_player_start, 3)
	elseif player_count == 3
		then
		Rule_AddOneShot(first_player_start, 3)
		Rule_AddOneShot(second_player_start, 3)
	elseif player_count == 4
		then
		Rule_AddOneShot(first_player_start, 3)
		Rule_AddOneShot(second_player_start, 3)
		Rule_AddOneShot(third_player_start, 3)
	elseif player_count == 5
		then
		Rule_AddOneShot(first_player_start, 3)
		Rule_AddOneShot(second_player_start, 3)
		Rule_AddOneShot(third_player_start, 3)
		Rule_AddOneShot(fourth_player_start, 3)
	elseif player_count == 6
		then
		Rule_AddOneShot(first_player_start, 3)
		Rule_AddOneShot(second_player_start, 3)
		Rule_AddOneShot(third_player_start, 3)
		Rule_AddOneShot(fourth_player_start, 3)
		Rule_AddOneShot(fifth_player_start, 3)
	elseif player_count == 7
		then
		Rule_AddOneShot(first_player_start, 3)
		Rule_AddOneShot(second_player_start, 3)
		Rule_AddOneShot(third_player_start, 3)
		Rule_AddOneShot(fourth_player_start, 3)
		Rule_AddOneShot(fifth_player_start, 3)
		Rule_AddOneShot(sixth_player_start, 3)
	elseif player_count == 8
		then
		Rule_AddOneShot(first_player_start, 3)
		Rule_AddOneShot(second_player_start, 3)
		Rule_AddOneShot(third_player_start, 3)
		Rule_AddOneShot(fourth_player_start, 3)
		Rule_AddOneShot(fifth_player_start, 3)
		Rule_AddOneShot(sixth_player_start, 3)
		Rule_AddOneShot(seventh_player_start, 3)
	else
		Rule_AddOneShot(first_player_start, 3)
	end
end
	
Scar_AddInit(OnInit)

function first_player_start()
	Player_SetResource(player1, RT_Wood, 1200)
	Player_SetResource(player1, RT_Food, 1000)
	Player_SetResource(player1, RT_Stone, 400)
	Player_SetResource(player1, RT_Gold, 200)
	Rule_AddOneShot(first_player_stream_one_start, 15)
	Rule_AddOneShot(first_player_stream_one_spawns_start, 15)
	Rule_AddOneShot(first_player_stream_two_spawns_start, 60)
	Rule_AddOneShot(first_player_stream_three_spawns_start, 120)
	Rule_AddOneShot(first_player_stream_four_spawns_start, 180)
	Rule_AddOneShot(first_player_stream_five_spawns_start, 240)
	Rule_AddOneShot(first_player_stream_six_spawns_start, 300)
	Rule_AddOneShot(art_move_start, 300)
	Rule_AddOneShot(first_player_art_wave_spawns_start_1, 300)
	Rule_AddOneShot(first_player_art_def_spawns_start_1, 360)
	Rule_AddOneShot(first_player_deathball_spawns_start_1, 300)
	Rule_AddOneShot(first_player_art_wave_spawns_start_2, 480)
	Rule_AddOneShot(first_player_art_def_spawns_start_2, 540)
	Rule_AddOneShot(first_player_deathball_spawns_start_2, 420)
	Rule_AddOneShot(first_player_stream_seven_spawns_start, 600)
	Rule_AddOneShot(first_player_stream_eight_spawns_start, 660)
	Rule_AddOneShot(first_player_stream_nine_spawns_start, 720)
	Rule_AddOneShot(first_player_stream_ten_spawns_start, 780)
	Rule_AddOneShot(first_player_stream_eleven_spawns_start, 840)
	Rule_AddOneShot(first_player_stream_twelve_spawns_start, 900)
	Rule_AddOneShot(first_player_art_wave_spawns_start_3, 900)
	Rule_AddOneShot(first_player_art_def_spawns_start_3, 960)
	Rule_AddOneShot(first_player_deathball_spawns_start_3, 900)
	Rule_AddOneShot(first_player_art_wave_spawns_start_4, 1080)
	Rule_AddOneShot(first_player_art_def_spawns_start_4, 1140)
	Rule_AddOneShot(first_player_deathball_spawns_start_4, 1020)
	Rule_AddOneShot(first_player_stream_thirteen_spawns_start, 1200)
	Rule_AddOneShot(first_player_stream_fourteen_spawns_start, 1260)
	Rule_AddOneShot(first_player_stream_fifteen_spawns_start, 1320)
	Rule_AddOneShot(first_player_stream_sixteen_spawns_start, 1380)
	Rule_AddOneShot(first_player_stream_seventeen_spawns_start, 1440)
	Rule_AddOneShot(first_player_stream_eighteen_spawns_start, 1500)
	Rule_AddOneShot(first_player_art_wave_spawns_start_5, 1500)
	Rule_AddOneShot(first_player_art_def_spawns_start_5, 1560)
	Rule_AddOneShot(first_player_deathball_spawns_start_5, 1500)
	Rule_AddOneShot(first_player_art_wave_spawns_start_6, 1680)
	Rule_AddOneShot(first_player_art_def_spawns_start_6, 1740)
	Rule_AddOneShot(first_player_deathball_spawns_start_6, 1620)
	Rule_AddOneShot(first_player_stream_nineteen_spawns_start, 1800)
	Rule_AddOneShot(first_player_stream_twenty_spawns_start, 1860)
	Rule_AddOneShot(first_player_stream_twentyone_spawns_start, 1920)
	Rule_AddOneShot(first_player_stream_twentytwo_spawns_start, 1980)
	Rule_AddOneShot(first_player_stream_twentythree_spawns_start, 2040)
	Rule_AddOneShot(first_player_stream_twentyfour_spawns_start, 2100)
	Rule_AddOneShot(first_player_art_wave_spawns_start_7, 2160)
	Rule_AddOneShot(first_player_stream_twentyfive_spawns_start, 2220)
	Rule_AddOneShot(first_player_stream_twentysix_spawns_start, 2280)
	local sg_villagers = SGroup_CreateIfNotFound("sg_villagers")
	local sg_spearman = SGroup_CreateIfNotFound("sg_spearman")
	local sg_archer = SGroup_CreateIfNotFound("sg_archer")
	player_civ1 = Player_GetRaceName(player1)
	if player_civ1 == "english" then
		unit_villager_one = BP_GetSquadBlueprint("unit_villager_1_eng")
		unit_spearman_one = BP_GetSquadBlueprint("unit_archer_2_eng")
		unit_archer_one = BP_GetSquadBlueprint("unit_archer_2_eng")
	elseif player_civ1 == "chinese" then
		unit_villager_one = BP_GetSquadBlueprint("unit_villager_1_chi")
		unit_spearman_one = BP_GetSquadBlueprint("unit_spearman_1_chi")
		unit_archer_one = BP_GetSquadBlueprint("unit_archer_2_chi")
	elseif player_civ1 == "french" then
		unit_villager_one = BP_GetSquadBlueprint("unit_villager_1_fre")
		unit_spearman_one = BP_GetSquadBlueprint("unit_spearman_1_fre")
		unit_archer_one = BP_GetSquadBlueprint("unit_archer_2_fre")
	elseif player_civ1 == "hre" then
		unit_villager_one = BP_GetSquadBlueprint("unit_villager_1_hre")
		unit_spearman_one = BP_GetSquadBlueprint("unit_spearman_1_hre")
		unit_archer_one = BP_GetSquadBlueprint("unit_archer_2_hre")
	elseif player_civ1 == "mongol" then
		unit_villager_one = BP_GetSquadBlueprint("unit_villager_1_mon")
		unit_spearman_one = BP_GetSquadBlueprint("unit_archer_2_mon")
		unit_archer_one = BP_GetSquadBlueprint("unit_archer_2_mon")
	elseif player_civ1 == "rus" then
		unit_villager_one = BP_GetSquadBlueprint("unit_villager_1_rus")
		unit_spearman_one = BP_GetSquadBlueprint("unit_spearman_1_rus")
		unit_archer_one = BP_GetSquadBlueprint("unit_archer_2_rus")
	elseif player_civ1 == "sultanate" then
		unit_villager_one = BP_GetSquadBlueprint("unit_villager_1_sul")
		unit_spearman_one = BP_GetSquadBlueprint("unit_spearman_1_sul")
		unit_archer_one = BP_GetSquadBlueprint("unit_archer_2_sul")
	elseif player_civ1 == "abbasid" then
		unit_villager_one = BP_GetSquadBlueprint("unit_villager_1_abb")
		unit_spearman_one = BP_GetSquadBlueprint("unit_spearman_1_abb")
		unit_archer_one = BP_GetSquadBlueprint("unit_archer_2_abb")
	end
		Util_CreateSquads(player1, sg_villagers, unit_villager_one, InitSpawnVil1, Move_InitSpawn1)
		Util_CreateSquads(player1, sg_villagers, unit_villager_one, InitSpawnVil1, Move_InitSpawn1)
		Util_CreateSquads(player1, sg_villagers, unit_villager_one, InitSpawnVil1, Move_InitSpawn1)
		Util_CreateSquads(player1, sg_villagers, unit_villager_one, InitSpawnVil1_2, Move_InitSpawnArmy1)
		Util_CreateSquads(player1, sg_villagers, unit_villager_one, InitSpawnVil1_2, Move_InitSpawnArmy1)
		Util_CreateSquads(player1, sg_spearman, unit_spearman_one, InitSpawnSpear1, Move_InitSpawnArmy1)
		Util_CreateSquads(player1, sg_spearman, unit_spearman_one, InitSpawnSpear1, Move_InitSpawnArmy1)
		Util_CreateSquads(player1, sg_spearman, unit_spearman_one, InitSpawnSpear1, Move_InitSpawnArmy1)
		Util_CreateSquads(player1, sg_archer, unit_archer_one, InitSpawnArc1, Move_InitSpawnArmy1)
		Util_CreateSquads(player1, sg_archer, unit_archer_one, InitSpawnArc1, Move_InitSpawnArmy1)
		Util_CreateSquads(player1, sg_archer, unit_archer_one, InitSpawnArc1, Move_InitSpawnArmy1)	
end

function second_player_start()
	Rule_AddOneShot(second_player_stream_one_start, 15)
	Rule_AddOneShot(second_player_stream_one_spawns_start, 15)
	Rule_AddOneShot(second_player_stream_two_spawns_start, 60)
	Rule_AddOneShot(second_player_stream_three_spawns_start, 120)
	Rule_AddOneShot(second_player_stream_four_spawns_start, 180)
	Rule_AddOneShot(second_player_stream_five_spawns_start, 240)
	Rule_AddOneShot(second_player_stream_six_spawns_start, 300)
	Rule_AddOneShot(second_player_deathball_spawns_start_1, 300)
	Rule_AddOneShot(second_player_deathball_spawns_start_2, 420)
	Rule_AddOneShot(second_player_stream_seven_spawns_start, 600)
	Rule_AddOneShot(second_player_stream_eight_spawns_start, 660)
	Rule_AddOneShot(second_player_stream_nine_spawns_start, 720)
	Rule_AddOneShot(second_player_stream_ten_spawns_start, 780)
	Rule_AddOneShot(second_player_stream_eleven_spawns_start, 840)
	Rule_AddOneShot(second_player_stream_twelve_spawns_start, 900)
	Rule_AddOneShot(second_player_deathball_spawns_start_3, 900)
	Rule_AddOneShot(second_player_deathball_spawns_start_4, 1020)
	Rule_AddOneShot(second_player_stream_thirteen_spawns_start, 1200)
	Rule_AddOneShot(second_player_stream_fourteen_spawns_start, 1260)
	Rule_AddOneShot(second_player_stream_fifteen_spawns_start, 1320)
	Rule_AddOneShot(second_player_stream_sixteen_spawns_start, 1380)
	Rule_AddOneShot(second_player_stream_seventeen_spawns_start, 1440)
	Rule_AddOneShot(second_player_stream_eighteen_spawns_start, 1500)
	Rule_AddOneShot(second_player_deathball_spawns_start_5, 1500)
	Rule_AddOneShot(second_player_deathball_spawns_start_6, 1620)
	Rule_AddOneShot(second_player_stream_nineteen_spawns_start, 1800)
	Rule_AddOneShot(second_player_stream_twenty_spawns_start, 1860)
	Rule_AddOneShot(second_player_stream_twentyone_spawns_start, 1920)
	Rule_AddOneShot(second_player_stream_twentytwo_spawns_start, 1980)
	Rule_AddOneShot(second_player_stream_twentythree_spawns_start, 2040)
	Rule_AddOneShot(second_player_stream_twentyfour_spawns_start, 2100)
	Rule_AddOneShot(second_player_stream_twentyfive_spawns_start, 2220)
	Rule_AddOneShot(second_player_stream_twentysix_spawns_start, 2280)
	local sg_villagers = SGroup_CreateIfNotFound("sg_villagers")
	local sg_spearman = SGroup_CreateIfNotFound("sg_spearman")
	local sg_archer = SGroup_CreateIfNotFound("sg_archer")
	player3 = World_GetPlayerAt(3)
	Player_SetResource(player3, RT_Wood, 1200)
	Player_SetResource(player3, RT_Food, 1000)
	Player_SetResource(player3, RT_Stone, 400)
	Player_SetResource(player3, RT_Gold, 200)
	player_civ2 = Player_GetRaceName(player3)
	if player_civ2 == "english" then
		unit_villager_two = BP_GetSquadBlueprint("unit_villager_1_eng")
		unit_spearman_two = BP_GetSquadBlueprint("unit_archer_2_eng")
		unit_archer_two = BP_GetSquadBlueprint("unit_archer_2_eng")
	elseif player_civ2 == "chinese" then
		unit_villager_two = BP_GetSquadBlueprint("unit_villager_1_chi")
		unit_spearman_two = BP_GetSquadBlueprint("unit_spearman_1_chi")
		unit_archer_two = BP_GetSquadBlueprint("unit_archer_2_chi")
	elseif player_civ2 == "french" then
		unit_villager_two = BP_GetSquadBlueprint("unit_villager_1_fre")
		unit_spearman_two = BP_GetSquadBlueprint("unit_spearman_1_fre")
		unit_archer_two = BP_GetSquadBlueprint("unit_archer_2_fre")
	elseif player_civ2 == "hre" then
		unit_villager_two = BP_GetSquadBlueprint("unit_villager_1_hre")
		unit_spearman_two = BP_GetSquadBlueprint("unit_spearman_1_hre")
		unit_archer_two = BP_GetSquadBlueprint("unit_archer_2_hre")
	elseif player_civ2 == "mongol" then
		unit_villager_two = BP_GetSquadBlueprint("unit_villager_1_mon")
		unit_spearman_two = BP_GetSquadBlueprint("unit_archer_2_mon")
		unit_archer_two = BP_GetSquadBlueprint("unit_archer_2_mon")
	elseif player_civ2 == "rus" then
		unit_villager_two = BP_GetSquadBlueprint("unit_villager_1_rus")
		unit_spearman_two = BP_GetSquadBlueprint("unit_spearman_1_rus")
		unit_archer_two = BP_GetSquadBlueprint("unit_archer_2_rus")
	elseif player_civ2 == "sultanate" then
		unit_villager_two = BP_GetSquadBlueprint("unit_villager_1_sul")
		unit_spearman_two = BP_GetSquadBlueprint("unit_spearman_1_sul")
		unit_archer_two = BP_GetSquadBlueprint("unit_archer_2_sul")
	elseif player_civ2 == "abbasid" then
		unit_villager_two = BP_GetSquadBlueprint("unit_villager_1_abb")
		unit_spearman_two = BP_GetSquadBlueprint("unit_spearman_1_abb")
		unit_archer_two = BP_GetSquadBlueprint("unit_archer_2_abb")
	end
		Util_CreateSquads(player3, sg_villagers, unit_villager_two, InitSpawnVil2, Move_InitSpawn2)
		Util_CreateSquads(player3, sg_villagers, unit_villager_two, InitSpawnVil2, Move_InitSpawn2)
		Util_CreateSquads(player3, sg_villagers, unit_villager_two, InitSpawnVil2, Move_InitSpawn2)
		Util_CreateSquads(player3, sg_villagers, unit_villager_two, InitSpawnVil2_2, Move_InitSpawnArmy2)
		Util_CreateSquads(player3, sg_villagers, unit_villager_two, InitSpawnVil2_2, Move_InitSpawnArmy2)
		Util_CreateSquads(player3, sg_spearman, unit_spearman_two, InitSpawnSpear2, Move_InitSpawnArmy2)
		Util_CreateSquads(player3, sg_spearman, unit_spearman_two, InitSpawnSpear2, Move_InitSpawnArmy2)
		Util_CreateSquads(player3, sg_spearman, unit_spearman_two, InitSpawnSpear2, Move_InitSpawnArmy2)
		Util_CreateSquads(player3, sg_archer, unit_archer_two, InitSpawnArc2, Move_InitSpawnArmy2)
		Util_CreateSquads(player3, sg_archer, unit_archer_two, InitSpawnArc2, Move_InitSpawnArmy2)
		Util_CreateSquads(player3, sg_archer, unit_archer_two, InitSpawnArc2, Move_InitSpawnArmy2)
end

function third_player_start()
	Rule_AddOneShot(third_player_stream_one_start, 15)
	Rule_AddOneShot(third_player_stream_one_spawns_start, 15)
	Rule_AddOneShot(third_player_stream_two_spawns_start, 60)
	Rule_AddOneShot(third_player_stream_three_spawns_start, 120)
	Rule_AddOneShot(third_player_stream_four_spawns_start, 180)
	Rule_AddOneShot(third_player_stream_five_spawns_start, 240)
	Rule_AddOneShot(third_player_stream_six_spawns_start, 300)
	Rule_AddOneShot(third_player_deathball_spawns_start_1, 300)
	Rule_AddOneShot(third_player_deathball_spawns_start_2, 420)
	Rule_AddOneShot(third_player_stream_seven_spawns_start, 600)
	Rule_AddOneShot(third_player_stream_eight_spawns_start, 660)
	Rule_AddOneShot(third_player_stream_nine_spawns_start, 720)
	Rule_AddOneShot(third_player_stream_ten_spawns_start, 780)
	Rule_AddOneShot(third_player_stream_eleven_spawns_start, 840)
	Rule_AddOneShot(third_player_stream_twelve_spawns_start, 900)
	Rule_AddOneShot(third_player_deathball_spawns_start_3, 900)
	Rule_AddOneShot(third_player_deathball_spawns_start_4, 1020)
	Rule_AddOneShot(third_player_stream_thirteen_spawns_start, 1200)
	Rule_AddOneShot(third_player_stream_fourteen_spawns_start, 1260)
	Rule_AddOneShot(third_player_stream_fifteen_spawns_start, 1320)
	Rule_AddOneShot(third_player_stream_sixteen_spawns_start, 1380)
	Rule_AddOneShot(third_player_stream_seventeen_spawns_start, 1440)
	Rule_AddOneShot(third_player_stream_eighteen_spawns_start, 1500)
	Rule_AddOneShot(third_player_deathball_spawns_start_5, 1500)
	Rule_AddOneShot(third_player_deathball_spawns_start_6, 1620)
	Rule_AddOneShot(third_player_stream_nineteen_spawns_start, 1800)
	Rule_AddOneShot(third_player_stream_twenty_spawns_start, 1860)
	Rule_AddOneShot(third_player_stream_twentyone_spawns_start, 1920)
	Rule_AddOneShot(third_player_stream_twentytwo_spawns_start, 1980)
	Rule_AddOneShot(third_player_stream_twentythree_spawns_start, 2040)
	Rule_AddOneShot(third_player_stream_twentyfour_spawns_start, 2100)
	Rule_AddOneShot(third_player_stream_twentyfive_spawns_start, 2220)
	Rule_AddOneShot(third_player_stream_twentysix_spawns_start, 2280)
	local sg_villagers = SGroup_CreateIfNotFound("sg_villagers")
	local sg_spearman = SGroup_CreateIfNotFound("sg_spearman")
	local sg_archer = SGroup_CreateIfNotFound("sg_archer")
	player4 = World_GetPlayerAt(4)
	Player_SetResource(player4, RT_Wood, 1200)
	Player_SetResource(player4, RT_Food, 1000)
	Player_SetResource(player4, RT_Stone, 400)
	Player_SetResource(player4, RT_Gold, 200)
	player_civ3 = Player_GetRaceName(player4)
	if player_civ3 == "english" then
		unit_villager_three = BP_GetSquadBlueprint("unit_villager_1_eng")
		unit_archer_three = BP_GetSquadBlueprint("unit_archer_2_eng")
	elseif player_civ3 == "chinese" then
		unit_villager_three = BP_GetSquadBlueprint("unit_villager_1_chi")
		unit_spearman_three = BP_GetSquadBlueprint("unit_spearman_1_chi")
		unit_archer_three = BP_GetSquadBlueprint("unit_archer_2_chi")
	elseif player_civ3 == "french" then
		unit_villager_three = BP_GetSquadBlueprint("unit_villager_1_fre")
		unit_spearman_three = BP_GetSquadBlueprint("unit_spearman_1_fre")
		unit_archer_three = BP_GetSquadBlueprint("unit_archer_2_fre")
	elseif player_civ3 == "hre" then
		unit_villager_three = BP_GetSquadBlueprint("unit_villager_1_hre")
		unit_spearman_three = BP_GetSquadBlueprint("unit_spearman_1_hre")
		unit_archer_three = BP_GetSquadBlueprint("unit_archer_2_hre")
	elseif player_civ3 == "mongol" then
		unit_villager_three = BP_GetSquadBlueprint("unit_villager_1_mon")
		unit_spearman_three = BP_GetSquadBlueprint("unit_archer_2_mon")
		unit_archer_three = BP_GetSquadBlueprint("unit_archer_2_mon")
	elseif player_civ3 == "rus" then
		unit_villager_three = BP_GetSquadBlueprint("unit_villager_1_rus")
		unit_spearman_three = BP_GetSquadBlueprint("unit_spearman_1_rus")
		unit_archer_three = BP_GetSquadBlueprint("unit_archer_2_rus")
	elseif player_civ3 == "sultanate" then
		unit_villager_three = BP_GetSquadBlueprint("unit_villager_1_sul")
		unit_spearman_three = BP_GetSquadBlueprint("unit_spearman_1_sul")
		unit_archer_three = BP_GetSquadBlueprint("unit_archer_2_sul")
	elseif player_civ3 == "abbasid" then
		unit_villager_three = BP_GetSquadBlueprint("unit_villager_1_abb")
		unit_spearman_three = BP_GetSquadBlueprint("unit_spearman_1_abb")
		unit_archer_three = BP_GetSquadBlueprint("unit_archer_2_abb")
	end
		Util_CreateSquads(player4, sg_villagers, unit_villager_three, InitSpawnVil3, Move_InitSpawn3)
		Util_CreateSquads(player4, sg_villagers, unit_villager_three, InitSpawnVil3, Move_InitSpawn3)
		Util_CreateSquads(player4, sg_villagers, unit_villager_three, InitSpawnVil3, Move_InitSpawn3)
		Util_CreateSquads(player4, sg_villagers, unit_villager_three, InitSpawnVil3_2, Move_InitSpawnArmy3)
		Util_CreateSquads(player4, sg_villagers, unit_villager_three, InitSpawnVil3_2, Move_InitSpawnArmy3)	
		Util_CreateSquads(player4, sg_spearman, unit_spearman_three, InitSpawnSpear3, Move_InitSpawnArmy3)
		Util_CreateSquads(player4, sg_spearman, unit_spearman_three, InitSpawnSpear3, Move_InitSpawnArmy3)
		Util_CreateSquads(player4, sg_spearman, unit_spearman_three, InitSpawnSpear3, Move_InitSpawnArmy3)
