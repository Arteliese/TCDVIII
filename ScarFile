		unit_villager_seven = BP_GetSquadBlueprint("unit_villager_1_fre")
		unit_spearman_seven = BP_GetSquadBlueprint("unit_spearman_1_fre")
		unit_archer_seven = BP_GetSquadBlueprint("unit_archer_2_fre")
